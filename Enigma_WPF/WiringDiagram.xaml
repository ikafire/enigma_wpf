            <Ellipse Name="ellipse_Left19" Style="{StaticResource leftEllipse}" Canvas.Top="475"/>
            <Ellipse Name="ellipse_Left20" Style="{StaticResource leftEllipse}" Canvas.Top="500"/>
            <Ellipse Name="ellipse_Left21" Style="{StaticResource leftEllipse}" Canvas.Top="525"/>
            <Ellipse Name="ellipse_Left22" Style="{StaticResource leftEllipse}" Canvas.Top="550"/>
