<UserControl x:Class="EnigmaWPF.WiringDiagram"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="645" Width="210">
    <UserControl.Resources>
        <Style TargetType="TextBlock" x:Key="typerStyle">
            <Setter Property="Width" Value="20"/>
            <Setter Property="Height" Value="25"/>
            <Setter Property="FontSize" Value="20"/>
            <Setter Property="TextAlignment" Value="Center"/>
            <Setter Property="FontFamily" Value="Courier New"/>
        </Style>
        <Style TargetType="Ellipse" x:Key="rightEllipse">
            <Setter Property="Width" Value="20"/>
            <Setter Property="Height" Value="20"/>
            <Setter Property="Fill" Value="SkyBlue"/>
            <Setter Property="Canvas.Left" Value="150"/>
            <EventSetter Event="MouseDown" Handler="ellipse_Right_MouseDown"/>
            <EventSetter Event="MouseEnter" Handler="ellipse_Right_MouseEnter"/>
            <EventSetter Event="MouseLeave" Handler="ellipse_Right_MouseLeave"/>
        </Style>
        <Style TargetType="Ellipse" x:Key="leftEllipse">
            <Setter Property="Width" Value="20"/>
            <Setter Property="Height" Value="20"/>
            <Setter Property="Fill" Value="SkyBlue"/>
            <Setter Property="Canvas.Left" Value="0"/>
            <EventSetter Event="MouseDown" Handler="ellipse_Left_MouseDown"/>
            <EventSetter Event="MouseEnter" Handler="ellipse_Left_MouseEnter"/>
            <EventSetter Event="MouseLeave" Handler="ellipse_Left_MouseLeave"/>
        </Style>
    </UserControl.Resources>
    <Grid>
        <Canvas Name="canvas_Main" Margin="0" Width="170" Height="645" Opacity="0.6">
            <Ellipse Name="ellipse_Left0" Style="{StaticResource leftEllipse}" Canvas.Top="0"/>
